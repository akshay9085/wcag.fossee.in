---
// /src/layouts/Layout.astro
export interface Props {
	title: string;
}
const { title } = Astro.props;

import 'bootstrap/dist/css/bootstrap.css'
import { Breadcrumb } from 'astro-bootstrap';
import 'css/custom.css'

---

<!DOCTYPE html>
<html lang="en">
	<head>
      <meta name="description" content="Mapathon">
      <meta name="author" content="FOSSEE web team">
      <meta name="keywords" content="Hackathon, OpenFOAM, CFD, OpenFoam, competition">
      <meta name="keywords"
      content="Fossee, IIT Bombay, Case Study, CFD, OpenFoam, Mapping, Open source, software">
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	  <script src="/cfd/static/cms/uploads/js/custom.js"></script>
  <!-- Global site tag (gtag.js) - Google Analytics --> <script async src="https://www.googletagmanager.com/gtag/js?id=analytics-code"></script> <script> window.dataLayer = window.dataLayer || []; function gtag(){dataLayer.push(arguments);} gtag('js', new Date()); gtag('config', 'analytics-code'); </script>
	</head>
	<body>
    <Breadcrumb />
		<slot />
	</body>
</html>